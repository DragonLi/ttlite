let thSum01 =
    forall (A :: *) (B :: *) (_ :: Sum A B) . Sum B A;

let proofSum01 =
    (\ (A :: *) (B :: *) (s :: Sum A B) ->
        sumElim A B
            (\ (_ :: Sum A B) -> Sum B A)
            (\ (a :: A) . InR B A a)
            (\ (b :: B) . InL B A b)
            s)
        :: thSum01;
