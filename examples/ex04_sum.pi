let thSum01 =
    forall (A :: *) (B :: *). Sum A B -> Sum B A;
let proofSum01 =
    (\ (A :: *) (B :: *) (s :: Sum A B) -> sumElim A B (\ (_ :: Sum A B) -> Sum B A) (InR B A) (InL B A) s)
        :: thSum01;

assume (X :: *);
assume (Y :: *);

assume (x :: X);
assume (y :: Y);

let lx = InL X Y x;
let ry = InR X Y y;

sumElim X Y (\ (_ :: Sum X Y) -> *) (\(_ :: X) -> *) (\(_ :: Y) -> *) lx;
sumElim X Y (\ (_ :: Sum X Y) -> *) (\(_ :: X) -> *) (\(_ :: Y) -> *) ry;
