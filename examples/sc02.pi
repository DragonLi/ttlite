let yyyNorm =
    (\ (a :: Nat) ->
        natElim
            (\ (b :: Nat) -> Nat)
            Zero
            (\ (b :: Nat) -> \ (c :: Nat) -> b)
            (natElim
                (\ (b :: Nat) -> forall (c :: Nat) . Nat)
                (\ (b :: Nat) -> b)
                (\ (b :: Nat) -> \ (c :: forall (c :: Nat) . Nat) -> \ (d :: Nat) -> Succ (c d))
                a
                Zero)) :: forall (a :: Nat) . Nat;

let yyySc =
    (\ (a :: Nat) ->
        natElim
            (\ (b :: Nat) -> Nat)
            Zero
            (\ (b :: Nat) -> \ (c :: Nat) ->
                natElim
                (\ (d :: Nat) -> Nat)
                Zero
                (\ (d :: Nat) -> \ (e :: Nat) -> Succ e)
                b) a) :: forall (a :: Nat) . Nat;

let proof =
    \(a :: Nat) ->
        natElim
            (\ (b :: Nat) -> Eq Nat (yyyNorm b) (yyySc b))
            (Refl Nat Zero);

