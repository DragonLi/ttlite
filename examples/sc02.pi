-- equality congruence for arguments
let cong1 =
  ( \ (a :: *) (b :: *) (f :: a -> b) ->
        eqElim a
            (\ (x :: a) (y :: a) (eq_x_y :: Eq a x y) -> Eq b (f x) (f y))
            (\ (x :: a) -> Refl b (f x)) )
  :: forall (a :: *) (b :: *) (f :: a -> b) (x :: a) (y :: a) . Eq a x y -> Eq b (f x) (f y);

let fcong1 =
  ( \ (a :: *) (b :: *) (x :: a) ->
        eqElim (a -> b)
            (\ (f :: a -> b) (g :: a -> b) (eq_f_g :: Eq (a -> b) f g) -> Eq b (f x) (g x))
            (\ (f :: a -> b) -> Refl b (f x)) )
  :: forall (a :: *) (b :: *) (x :: a) (f :: a -> b) (g :: a -> b) . Eq (a -> b) f g -> Eq b (f x) (g x);

let fargCong =
  ( \ (a :: *) (b :: *) (x :: a) (y :: a) (f :: a -> b) (g :: a -> b) (eq_x_y :: Eq a x y) ->
        eqElim (a -> b)
            (\ (f :: a -> b) (g :: a -> b) (eq_f_g :: Eq (a -> b) f g) ->  Eq b (f x) (g y))
            (\ (f :: a -> b) -> cong1 a b f x y eq_x_y)
            f
            g) ::
  forall (a :: *)
         (b :: *)
         (x :: a)
         (y :: a)
         (f :: a -> b)
         (g :: a -> b) .
         Eq a x y -> Eq (a -> b) f g -> Eq b (f x) (g y);

let fargCong2 =
   (\ (a :: *)
      (b :: *)
      (c :: *)
      (f :: a -> b -> c)
      (x1 :: a)
      (x2 :: a)
      (y1 :: b)
      (y2 :: b)
      (eq_x :: Eq a x1 x2)
      (eq_y :: Eq b y1 y2)
        ->
        Nat);

